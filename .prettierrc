{
    "plugins": [
        "prettier-plugin-astro",
        "prettier-plugin-tailwindcss"
    ],
    "pluginSearchDirs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "printWidth": 100,
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        }
    ]
}